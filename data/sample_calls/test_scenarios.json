  "violation_types": {
    "bindingstid_missing": {
      "severity": "high",
      "description": "Failure to disclose binding period",
      "norwegian_law": "Forbrukeravtaleloven § 18",
      "typical_phrases": [
        "standard abonnement",
        "vanlig periode",
        "ikke noe å bekymre seg for"
      ]
    },
    "price_incomplete": {
      "severity": "high", 
      "description": "Incomplete price information",
      "norwegian_law": "Markedsføringsloven § 6",
      "typical_phrases": [
        "bare X kroner",
        "ingen ekstra kostnader",
        "alt inkludert"
      ]
    },
    "excessive_pressure": {
      "severity": "medium",
      "description": "High-pressure sales tactics",
      "norwegian_law": "Markedsføringsloven § 9",
      "typical_phrases": [
        "må bestemme deg nå",
        "tilbudet utløper",
        "bare i dag"
      ]
    },
    "gdpr_violation": {
      "severity": "very_high",
      "description": "Improper data collection",
      "norwegian_law": "Personvernloven (GDPR)",
      "typical_phrases": [
        "gi meg fødselsnummer",
        "bankkortdetaljer",
        "all personlig informasjon"
      ]
    },
    "false_marketing": {
      "severity": "very_high",
      "description": "False or misleading claims",
      "norwegian_law": "Markedsføringsloven § 3",
      "typical_phrases": [
        "du har vunnet",
        "beste tilbud på markedet",
        "alle er fornøyde"
      ]
    }
  },

  "compliance_indicators": {
    "positive_signals": [
      "bindingstid på X måneder",
      "totalpris inkludert merverdiavgift",
      "du har angrerett",
      "kan jeg sende informasjon på e-post",
      "ta tiden du trenger",
      "spørsmål om noe er uklart"
    ],
    "negative_signals": [
      "må bestemme deg nå",
      "tilbudet utløper",
      "ingen ekstra kostnader",
      "standard periode",
      "ikke noe å bekymre seg for",
      "alle er fornøyde"
    ]
  },

  "test_scenarios": [
    {
      "scenario_id": "perfect_compliance",
      "name": "Perfect Compliance Call",
      "description": "Demonstrates ideal Norwegian telecom sales call",
      "expected_score": "9-10",
      "key_elements": [
        "Clear bindingstid disclosure",
        "Complete price transparency", 
        "No sales pressure",
        "GDPR compliant data collection",
        "Offers written confirmation"
      ],
      "file_example": "good_call_compliant.txt"
    },
    {
      "scenario_id": "multiple_violations",
      "name": "Multiple Serious Violations",
      "description": "Call with several compliance breaches",
      "expected_score": "1-3",
      "key_elements": [
        "No bindingstid disclosure",
        "Incomplete pricing",
        "High pressure tactics",
        "GDPR violations",
        "False marketing claims"
      ],
      "file_example": "bad_call_violations.txt"
    },
    {
      "scenario_id": "bindingstid_avoidance",
      "name": "Binding Period Avoidance",
      "description": "Agent actively avoids binding period questions",
      "expected_score": "3-4",
